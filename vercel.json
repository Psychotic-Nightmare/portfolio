{
  "buildCommand": "cd portfolio && npm install && npm run build",
  "outputDirectory": "portfolio/build/client",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/server"
    }
  ],
  "functions": {
    "portfolio/api/server.js": {
      "runtime": "nodejs20.x",
      "includeFiles": "portfolio/build/server/**"
    }
  }
}
